{
	"dependencies": {
		"maplibre-gl": "^5.13.0"
	},
	"devDependencies": {
		"@rspack/cli": "^1.6.7",
		"@rspack/core": "^1.6.7",
		"@tsconfig/node24": "^24.0.1",
		"@tsconfig/recommended": "^1.0.10",
		"@typescript/native-preview": "^7.0.0-dev.20251210.1",
		"@types/node": "^24.9.1",
		"concurrently": "^9.2.1",
		"nodemon": "^3.1.11"
	},
	"name": "skytracker",
	"license": "MIT",
	"scripts": {
		"build:api": "npm run build -w @skytracker/api",
		"build:web": "npm run build -w @skytracker/web",
		"build": "npm run build:api && npm run build:web",
		"dev:api": "npm run dev -w @skytracker/api",
		"dev:web": "npm run dev -w @skytracker/web",
		"dev": "concurrently -n 'API,WEB' -c 'bgBlue.black,bgMagentaBright.black' 'npm run dev:api' 'npm run dev:web' & nodemon dist/main.js"
	},
	"version": "0.0.1",
	"workspaces": ["apps/*"]
}
